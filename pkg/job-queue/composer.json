{
    "name": "enqueue/job-queue",
    "type": "library",
    "description": "Job Queue",
    "keywords": ["messaging", "queue", "jobs"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:php-enqueue/test.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^2.8|^3",
        "enqueue/enqueue": "dev-master",
        "doctrine/orm": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "enqueue/test": "dev-master",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/browser-kit": "^2.8|^3",
        "symfony/expression-language": "^2.8|^3"
    },
    "autoload": {
        "psr-4": { "Enqueue\\JobQueue\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
