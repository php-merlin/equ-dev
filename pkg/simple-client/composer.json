{
    "name": "enqueue/simple-client",
    "type": "library",
    "description": "Message Queue Simple Client",
    "keywords": ["messaging", "queue", "amqp", "rabbitmq"],
    "homepage": "https://github.com/php-enqueue/enqueue-dev",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "enqueue/enqueue": "^0.8@dev",
        "symfony/dependency-injection": "^2.8|^3|^4",
        "symfony/config": "^2.8|^3|^4",
        "symfony/console": "^2.8|^3|^4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "enqueue/test": "^0.8@dev",
        "enqueue/amqp-ext": "^0.8@dev",
        "enqueue/fs": "^0.8@dev",
        "enqueue/null": "^0.8@dev"
    },
    "autoload": {
        "psr-4": { "Enqueue\\SimpleClient\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.8.x-dev"
        }
    }
}
