{
    "name": "enqueue/fs",
    "type": "library",
    "description": "Enqueue Filesystem based transport",
    "keywords": ["messaging", "queue", "filesystem", "local"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:php-enqueue/test.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "enqueue/psr-queue": "^0.3|0.4@dev",
        "symfony/filesystem": "^2.8|^3",
        "makasim/temp-file": "^0.2",
        "psr/log": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "enqueue/enqueue": "^0.3|0.4@dev",
        "enqueue/null": "^0.3|0.4@dev",
        "enqueue/test": "^0.3|0.4@dev",
        "symfony/dependency-injection": "^2.8|^3",
        "symfony/config": "^2.8|^3"
    },
    "autoload": {
        "psr-4": { "Enqueue\\Fs\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
